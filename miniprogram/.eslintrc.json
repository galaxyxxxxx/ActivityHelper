{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [],
    "globals": {
        "App": true,
        "Page": true,
        "Component": true,
        "Behavior": true,
        "wx": true,
        "getApp": true,
        "getCurrentPages": true,
        "$1": true,
        "$2": true,
        "$3": true,
        "$4": true,
        "$5": true,
        "$6": true,
        "$7": true,
        "$8": true,
        "$9": true,
        "lastMatch": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": "warn"
    }
}