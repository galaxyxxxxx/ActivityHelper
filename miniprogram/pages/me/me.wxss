page {
  font-size: 25rpx;
  background-color: white;
}

van-icon {
  vertical-align: middle
}

/* 顶部栏 | 请慎重展开该样式 */
.intro {
  width: 100%;
  height: 225rpx;
  /* 由于微信小程序不支持本地图片作为背景图 因此进行了base64转码 */
  background: url(data:image/svg+xml;base64,) no-repeat, linear-gradient(to bottom, var(--color-background), white);
  background-size: 173% 100%;
}

.cover {
  position: absolute;
  top: 150rpx;
  left: 12%;
  width: 150rpx;
  height: 150rpx;
  border-radius: 100rpx;
  border: 1rpx solid transparent;
  overflow: hidden;
}

.name {
  position: absolute;
  top: 90rpx;
  left: 210rpx;
  font-size: var(--font-size-M);
}

.edit_setting {
  width: auto;
  position: absolute;
  top: 230rpx;
  left: 65%;
}

.setting {
  display: inline-block;
  margin-left: 15rpx;
}

/* tab的样式 */
.tabNav {
  letter-spacing: 3rpx;
}

.newAct {
  z-index: 5;
  position: fixed;
  top: 80%;
  left: 79%;
  opacity: 0.9;
}

.acts {
  position: relative;
  top: 100rpx;
}

.act {
  /* border-radius: var(--font-size-M); */
  position: relative;
  top: -5rpx;
  overflow: hidden;
  background-color: rgb(255, 255, 255);
  margin: 0 30rpx 10rpx 30rpx;
  padding: 30rpx;
  border-top: 1rpx solid #dddddd;
  /* box-shadow: 10rpx 10rpx 40rpx rgba(150, 150, 150, 0.2); */
}

.act .actLable {
  margin-top: 10rpx;
  letter-spacing: 1rpx;
  font-weight: 300;
  color: #868688;
}

.act .actMore{
  margin-top:-13rpx;
  margin-right: -10rpx;
  display: inline;
  float: right;
  color: #868688;
}

.act .actTitle {
  display: inline-block;
  letter-spacing: 2rpx;
  margin-top: 15rpx;
  font-weight: 600;
  font-size: 30rpx;
}

.act .actIntro {
  margin-top: 15rpx;
  font-size: var(--font-size-S);
  color: #868688;
  letter-spacing: 5rpx;
}

.act .actAddr {
  display: inline;
}

.act .actHost {
  display: inline;
  margin-left: 10%;
  position: relative;
  float: right;
}
.actLable .collect {
  display: inline;
  float: right;
  position: relative;
  top: -10rpx;
}

.actLable .collect::after{
  left: -20rpx; 
  top: -20rpx; 
  right: -20rpx; 
  bottom: -20rpx;
}