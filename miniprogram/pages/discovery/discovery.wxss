page{
  background-color: white;
  font-size: 25rpx;
}

.contain {
  width: 100%;
  height: 100%;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}

.week {
  padding: 0;
  width: 100%;
  margin: 0 auto;
  /* border-bottom: transparent solid 1px; */
  background-color: white;
}
.week>view{
  position: relative;
  display: inline-block;
  width: 14.285%;
  /* width: 12.57%; */
  text-align: center;
  vertical-align: middle;
}
.weekMark{
  font-size: 8pt;
}
.weekend {
  color: #b6b6b6;
}

.calendar {
  --calendar-height: 250px;
}

.date-head{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 12pt;
  color: #000000;
  /* border-top: 1px solid transparent; */
}
.actDay .date-head{
  color: black;
}
.nowDay .date-head{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  text-align: center;
  color: white;
  background-color: red;
  margin: 0 auto;
  margin-bottom: 20rpx; 
}

.lastMonth{
  z-index: 1000;
  position: absolute;
  top: 400rpx;
  left: 0;
  background-color: aqua;
}


.nextMonth{
  z-index: 1000;
  position: absolute;
  top: 400rpx;
  left: 90%;
  background-color: aqua;
}

.activity{
  width: 100%;
  background-color: white;
  border-top: #e4e2e2 1px solid;
}
.noEvents{
  text-align: center;
  padding-top: 20%;
  color: #939395;
}